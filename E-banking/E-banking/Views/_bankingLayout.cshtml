<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Home - E-Banking</title>
    <!--  jQuery  -->

    <script src="~/Scripts/jquery-1.11.0.min.js"></script>

    <!-- Bootstrap core CSS -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/normalize.css" rel="stylesheet" />


    <!-- Owl stylesheet -->
    <link href="~/Scripts/owl-carousel/owl.carousel.css" rel="stylesheet" />
    <link href="~/Scripts/owl-carousel/owl.theme.css" rel="stylesheet" />


    <!-- Custom styles for this template -->
    <link href="~/Content/css/main.css" rel="stylesheet" />
    <link href="~/Content/css/responsive.css" rel="stylesheet" />
    <link rel="shortcut icon" href="~/Content/Img/icon-ebanking.png">

    <!-- Font -->
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,300,100italic,300italic,400italic,500,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>

    <!-- Icons Fonts-->
    <link href="~/Content/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />



    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>


<body>

    <!--Top Header-->

    <div class="top-header menu-scroll">
        <div class="container">
            <div class="row">

                <!-- Mobile Icon-->
                <div class="mobile-icon" id="mobile-icon">
                    <i class="fa fa-bars"></i>
                </div>
                <!-- Primary Page Layout


                    <!-- BEGIN WRAPPER -->
                @*             <!-- BEGIN HEADER -->*@
                <header>

                    <!--Top Header-->
                    <div id="top-header">
                        <div class="container">
                            <div class="col-sm-4">
                                <div class="logo-container">
                                    <figure><a href="#"><img src="~/Content/Img/logo-ebanking.png"></a></figure>
                                </div>
                            </div>

                            <div class="col-lg-8 ">

                                <div class="#">

                                    <ul class="nav pull-right navbar-nav">
                                        <li class="main-menu-top">@Html.ActionLink("Home", "Index")</li>
                                        <li class="main-menu-top"><a href="#">Sobre Nosotros</a></li>
                                        <li class="main-menu-top"><a href="#contactos">Contact&eacute;nos</a></li>
                                    </ul>
                                </div>
                            </div>

                            <div class="col-sm-12">
                                <nav class="mobile-menu">
                                    <ul class="nav pull-right navbar-nav">
                                        <li class="main-menu-top"><a href="#">@Html.ActionLink("Home", "Index")</a></li>
                                        <li class="main-menu-top"><a href="#">Sobre Nosotros</a></li>
                                        <li class="main-menu-top"><a href="#contactos">Contact&eacute;nos</a></li>
                                    </ul>
                                </nav>
                            </div>

                        </div>

                        <!--Mobile Menu-->
                     <!-- End Menu Left-->

                    </div>
                </header>
            </div>
        </div>
    </div>

    <section class="cl-md-padding-both" id="header">

        <div class="container">
            <div class="col-sm-8">

                <div class="container">
                    <div class="form-group">
                        <h1 class="h-set">E-Banking Transaciones</h1>
                    </div>

                    <div class="form-group">
                        <nav class="menu-left">
                            <ul class="nav pull-left navbar-nav">
                                <li class="main-menu-top"><a href="#">Sobre Nosotros</a></li>
                                <li class="main-menu-top"><a href="#">Mensajes</a></li>
                                <li class="main-menu-top"><a href="#">Configurar</a></li>
                                <li class="main-menu-top"><a href="#">Ayuda</a></li>
                                <li class="main-menu-top"><a href="#contactos">Salir</a></li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
            <div class="col-sm-4">
                <!-- BEGIN SEARCH FORM -->
                <form id="search-form" class="search-form" action="#">
                    <input type="text" placeholder="Buscar ..." />
                    <input type="submit" value="" />
                </form>
                <!-- END SEARCH FORM -->
            </div>
        </div>

    </section>

    <!--Top Header-->
    <!--Body Section-->
    @RenderBody()

    <div class="container">

        <div class="main-section cl-lg-padding-both">
            <div class="container">
                <div class="col-sm-4">
                    <div class="container">

                        <div class="sidebar">
                            <div class="sidebar-left">
                                <label class="label-menu">Transaciones</label>
                            </div>
                        </div>
                        
                        <ul class="menu-sidebar menu-right">

                            <li><a href="#">Pagos</a></li>
                            <li><a href="#">Transferencias</a></li>
                            <li><a href="#">Desenbolsos</a></li>
                        </ul>

                        <div class="sidebar">
                            <div class="sidebar-left">
                                <label class="label-menu">Solicitudes</label>
                            </div>
                        </div>

                   
                        <ul class="menu-sidebar">

                            <li><a href="#">Solicitudes</a></li>
                            <li><a href="#">Beneficiarios</a></li>
                            <li><a href="#">Millas E-banking</a></li>
                        </ul>
                        <div class="sidebar">
                            <div class="sidebar-left">
                                <label class="label-menu">Consultas</label>
                            </div>
                        </div>
                        <ul class="menu-sidebar">

                            <li><a href="#">Cuentas</a></li>
                            <li><a href="#">Tarjetas</a></li>
                            <li><a href="#">Pr&eacute;stamos</a></li>
                            <li><a href="#">Certificados</a></li>
                        </ul>
                    </div>

                </div>

                <div class="col-sm-8">
                    
                        @RenderSection("BankSection", false)
                    

                </div>
            </div>
        </div>
    </div>


        <!-- End Servicios Section-->
        <!-- Conatcto Section-->
        <footer id="contactos" class="contacto-section cl-md-padding-both">
            <div class="container">

            </div>

        </footer><!-- End Conatcto Section-->
        <!-- Copyright Section-->
        <div class="copyright">
            <div class="container">
                <div class="row">
                    <div class="colsm-12">
                        <p>© @DateTime.Now.Year - @Html.ActionLink("E-Banking", "Index", null, new { @class = "hightlight" }). Todos los derechos reservados</p>
                    </div>
                </div>
            </div>
        </div><!-- End Copyright Section-->
        <script src="~/Scripts/bootstrap.min.js"></script>


        <!-- Owl js-->
        <script src="~/Scripts/owl-carousel/owl.carousel.js"></script>

        <!--  Custum jQuery  -->
        <script src="~/Scripts/custom.js"></script>



</body>
</html>

